{
  "main": "./dist/binding.js",
  "types": "./dist/binding.d.ts",
  "dependencies": {
    "node-gyp-build": "^4.8.2"
  },
  "scripts": {
    "prepare": "tsc",
    "build": "prebuildify --napi --strip",
    "postbuild": "tsc",
    "install": "node-gyp-build",
    "test": "jest"
  },
  "private": false,
  "gypfile": true,
  "name": "@jamiephan/casclib",
  "version": "2.0.0-dev.5",
  "description": "CascLib node bindings",
  "author": "Joe Istas <joe.istas@istastech.com>",
  "license": "MIT",
  "repository": "github:joeistas/node-casclib",
  "devDependencies": {
    "@types/jest": "^22.2.0",
    "@types/node": "^7.0.22",
    "jest": "^22.4.2",
    "node-addon-api": "^1.1.0",
    "prebuildify": "^6.0.1",
    "ts-jest": "^22.4.1",
    "typescript": "^2.3.3"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
